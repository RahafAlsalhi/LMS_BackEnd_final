{
  "name": "lms-backend",
  "version": "1.0.0",
  "description": "Learning Management System Backend API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:debug": "nodemon --inspect server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:test": "node scripts/test-db.js",
    "db:seed": "node scripts/seed-db.js",
    "lint": "echo \"Linting not configured\"",
    "clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "nodemonConfig": {
    "watch": [
      "server.js",
      "routes/",
      "middleware/",
      "controllers/",
      "config/",
      ".env"
    ],
    "ext": "js,json",
    "ignore": [
      "node_modules/",
      "uploads/",
      "logs/"
    ],
    "delay": "1000"
  },
  "keywords": [
    "lms",
    "education",
    "express",
    "nodejs",
    "api",
    "learning-management"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "sequelize": "^6.35.2",
    "multer": "^1.4.5-lts.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
